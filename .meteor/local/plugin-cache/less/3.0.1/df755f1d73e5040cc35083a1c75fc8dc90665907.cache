lf {\n          width: 200px;\n          background-color: var(--lightturquoise);\n\n\n          .name {\n            font-weight: 600;\n            font-size: 17px;\n            line-height: 20px;\n            color: #4a5568;\n            margin-bottom: 10px;\n            overflow-wrap: anywhere;\n          }\n\n          .text {\n            font-size: 17px;\n            line-height: 20px;\n            color: #718096;\n            overflow-wrap: anywhere;\n          }\n\n          .timeStamp {\n            margin-top: 10px;\n            font-size: 14px;\n            line-height: 17px;\n            color: #c2cbd3;\n          }\n        }\n\n        .text-container-other {\n          width: 200px;\n          background-color: var(--chat-selfe);\n\n\n          .name {\n            font-weight: 600;\n            font-size: 17px;\n            line-height: 20px;\n            color: #4a5568;\n            margin-bottom: 10px;\n            overflow-wrap: anywhere;\n          }\n\n          .text {\n            font-size: 17px;\n            line-height: 20px;\n            color: #718096;\n            overflow-wrap: anywhere;\n          }\n\n          .timeStamp {\n            margin-top: 10px;\n            font-size: 14px;\n            line-height: 17px;\n            color: #c2cbd3;\n          }\n        }\n\n      }\n    }\n\n    form {\n      width: 100%;\n      padding: 0 14px 15px 14px;\n\n      .chat-footer {\n        display: flex;\n        align-items: flex-start;\n\n        .chat-input {\n          outline: none;\n          border: 1px solid #e2e5e9;\n          border-radius: 5px 0px 0px 5px;\n          resize: none;\n          overflow: auto;\n          color: #718096;\n          font-size: 14px;\n          font-weight: 400;\n          flex-grow: 2;\n          padding: 6px;\n        }\n\n        .chat-button-send {\n          background-color: #ffffff;\n          border: 1px solid #e2e5e9;\n          border-left: 0;\n          box-sizing: border-box;\n          bor