 height: 100vh;\n    z-index: 50;\n\n    .chat {\n      position: absolute;\n      bottom: 0px;\n      right: 160px;\n      pointer-events: all;\n      width: 270px;\n      height: auto;\n      max-height: calc(~\"100vh - 50px\");\n      grid-template-rows: 36px;\n\n      &.open {\n        height: 500px;\n        grid-template-rows: 36px 1fr auto;\n      }\n\n      .messages,\n      form {\n        background-color: white;\n        border: 1px solid #c0c2c9;\n        border-top: none;\n        border-bottom: none;\n      }\n\n      form {\n        background-color: white;\n      }\n    }\n  }\n\n  .chat {\n    width: 100%;\n    height: 100%;\n    display: grid;\n    grid-template-rows: 1fr auto;\n\n    .header {\n      background-color: var(--darkblue);\n      border: none;\n      color: white;\n      display: flex;\n      justify-content: space-between;\n      align-items: center;\n      padding: 0 10px 0 14px;\n\n      .title {\n        font-family:\"Trebuchet MS\", Arial, Helvetica, sans-serif; \n        font-size:14px; \n        font-weight:bold;\n        padding: 8px 0px 8px 0px;\n\n        &:hover {\n         text-decoration: 2px solid white underline;\n        }\n      }\n\n      .close-button {\n        justify-content: center;\n        align-items: center;\n        cursor: pointer;\n        padding: 8px 0px 8px 0px;\n      }\n    }\n\n    .messages {\n      width: 100%;\n      max-height: 300px;\n      overflow-y: auto;\n      overflow-x: hidden;\n      background-color: var(--darkblue);\n      padding: 10px 15px 10px 15px;\n\n      .empty {\n        margin-top: 20px;\n        color: #718096;\n        text-align: center;\n      }\n\n      .message {\n        display: flex;\n        flex-direction: column;\n        margin: 10px 0px 0px 0px;\n        justify-content: flex-end;\n\n        .avatar {\n          width: 1.3em;\n          height: 1.3em;\n          margin-right: 15px;\n        }\n\n        .text-container-self {\n          width: 200px;\n          background-color: var(--chat-self-color);\n          align-items: flex-end;\n          margin-left: 15px;\n\n          .name {\n            font-weight: 600;\n            font-size: 17px;\n            line-height: 20px;\n            color: #4a5568;\n            margin-bottom: 10px;\n            overflow-wrap: anywhere;\n          }\n\n          .text {\n            font-size: 17px;\n            line-height: 20px;\n            color: black;\n            overflow-wrap: anywhere;\n            text-align: left;\n            padding: 10px 10px 10px 10px;\n          }\n\n          .timeStamp {\n            margin-top: 10px;\n            font-size: 14px;\n            line-height: 17px;\n            color: #c2cbd3;\n          }\n        }\n\n        .text-container-other {\n          width: 200px;\n          background-color: var(--chat-other-color);\n\n\n          .name {\n            font-weight: 600;\n            font-size: 17px;\n            line-height: 20px;\n            color: #4a5568;\n            margin-bottom: 10px;\n            overflow-wrap: anywhere;\n          }\n\n          .text {\n            font-size: 17px;\n            line-height: 20px;\n            color: black;\n            overflow-wrap: anywhere;\n            text-align:left;\n            padding: 10px 10px 10px 10px;\n          }\n\n          .timeStamp {\n            margin-top: 10px;\n            font-size: 14px;\n            line-height: 17px;\n            color: #c2cbd3;\n          }\n        }\n\n      }\n\n      .other-message-tick-container {\n        width: 200px;\n        display: flex;\n        justify-content: flex-start;\n      }\n\n      .self-message-tick-container {\n        width: 225px;\n        padding: 0px 10px;\n        display: flex;\n        justify-content: flex-end;\n      }\n    }\n  }\n}\n\n.chat-footer-form {\n  display: flex;\n  width: 250px;\n  padding: 0 14px 0px 14px;\n  background-color: var(--darkblue);\n\n  .chat-footer {\n    display: flex;\n    align-items: flex-start;\n    padding-bottom: 15px;\n\n    .chat-input {\n      outline: none;\n      border: none;\n      resize: none;\n      overflow: auto;\n      color: white;\n      font-size: 14px;\n      font-weight: 400;\n      flex-grow: 2;\n      padding: 6px;\n      width: 220px;\n      background-color: rgb(28, 44, 82);\n    }\n  }\n\n  .chat-button-send {\n    background-color: var(--darkblue);\n    border: none;\n    border-radius: 0px;\n    box-sizing: border-box;\n    display: inline-block;\n    cursor: pointer;\n    padding: 0 10px 0px 0px;\n    vertical-align: middle;\n    font-size: 14px;\n    width: 40px;\n    height: 100%;\n    margin-left: 15px;\n  }\n}\n"]}}