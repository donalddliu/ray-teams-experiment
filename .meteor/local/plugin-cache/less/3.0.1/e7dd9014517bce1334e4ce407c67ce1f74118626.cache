n  }\n\n  .chat {\n    width: 100%;\n    height: 100%;\n    display: grid;\n    grid-template-rows: 1fr auto;\n\n    .header {\n      background-color: white;\n      border: 1px solid #c0c2c9;\n      border-radius: 8px 8px 0px 0px;\n      color: #c2cbd3;\n      display: flex;\n      align-items: center;\n      padding: 0 17px 0 14px;\n\n      .title {\n        flex-grow: 2;\n        font-weight: bold;\n        letter-spacing: 0.1em;\n      }\n\n      .close-button {\n        font-weight: bold;\n        font-size: 2rem;\n        cursor: pointer;\n      }\n    }\n\n    .messages {\n      height: 100%;\n      width: 100%;\n      overflow-y: auto;\n      overflow-x: hidden;\n      background-color: var(--darkblue);\n\n      .empty {\n        margin-top: 20px;\n        color: #718096;\n        text-align: center;\n      }\n\n      .message {\n        display: flex;\n        padding-left: 15px;\n        margin: 15px 24px 0px 0;\n\n        .avatar {\n          width: 1.3em;\n          height: 1.3em;\n          margin-right: 15px;\n        }\n\n        .text-container-self {\n          width: 200px;\n          background-color: var(--chat-self-color);\n          margin-left: 10%;\n\n          .name {\n            font-weight: 600;\n            font-size: 17px;\n            line-height: 20px;\n            color: #4a5568;\n            margin-bottom: 10px;\n            overflow-wrap: anywhere;\n          }\n\n          .text {\n            font-size: 17px;\n            line-height: 20px;\n            color: black;\n            overflow-wrap: anywhere;\n            text-align: left;\n            padding: 10px 10px 10px 10px;\n          }\n\n          .timeStamp {\n            margin-top: 10px;\n            font-size: 14px;\n            line-height: 17px;\n            color: #c2cbd3;\n          }\n        }\n\n        .text-container-other {\n          width: 200px;\n          background-color: var(--chat-other-color);\n\n\n          .name {\n            font-weight: 600;\n            font-size: 17px;\n            line-height: 20px;\n            color: #4a5568;\n            margin-bottom: 10px;\n            overflow-wrap: anywhere;\n          }\n\n          .text {\n            font-size: 17px;\n            line-height: 20px;\n            color: black;\n            overflow-wrap: anywhere;\n            text-align:left;\n            padding: 10px 10px 10px 10px;\n          }\n\n          .timeStamp {\n            margin-top: 10px;\n            font-size: 14px;\n            line-height: 17px;\n            color: #c2cbd3;\n          }\n        }\n\n      }\n\n      .self-message-tick-container {\n        width: 200px;\n        display: flex;\n        justify-content: flex-end;\n\n        .self-message-tick {\n            content:\" \"; \n            width:16px; \n            height:7px; \n            background:url(../../public/images/my-bubble.png) top left no-repeat; \n            background-size:16px 7px; \n            display:inline-block; \n            position:relative; \n            bottom:-7px; right:0\n        }\n      }\n    }\n\n    form {\n      width: 100%;\n      padding: 0 14px 15px 14px;\n\n      .chat-footer {\n        display: flex;\n        align-items: flex-start;\n\n        .chat-input {\n          outline: none;\n          border: 1px solid #e2e5e9;\n          border-radius: 5px 0px 0px 5px;\n          resize: none;\n          overflow: auto;\n          color: #718096;\n          font-size: 14px;\n          font-weight: 400;\n          flex-grow: 2;\n          padding: 6px;\n        }\n\n        .chat-button-send {\n          background-color: #ffffff;\n          border: 1px solid #e2e5e9;\n          border-left: 0;\n          box-sizing: border-box;\n          border-radius: 0px 5px 5px 0px;\n          display: inline-block;\n          cursor: pointer;\n          padding: 0 10px;\n          vertical-align: middle;\n          font-size: 14px;\n          width: 60px;\n          margin: unset;\n        }\n      }\n    }\n 