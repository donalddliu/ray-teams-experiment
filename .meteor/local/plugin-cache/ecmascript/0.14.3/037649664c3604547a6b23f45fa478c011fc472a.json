Nodes.push(\"\".concat(p.get(\"nodeId\")));\n  });\n  game.players.forEach(p => {\n    if (p.online === true && !p.get(\"inactve\")) {\n      activeNodes.push(\"\".concat(p.get(\"nodeId\")));\n    }\n  });\n  game.players.forEach(p => {\n    const newNeighbors = activeNodes.filter(id => parseInt(id) !== p.get(\"nodeId\"));\n    p.set(\"neighbors\", newNeighbors);\n    console.log(p);\n    console.log(p.get(\"neighbors\"));\n  });\n}","map":{"version":3,"sources":["server/util.js"],"names":["module","export","getNeighbors","getFullyConnectedLayer","structure","player","neighbors","network","split","forEach","n","connection","playerIndex","get","parseInt","push","_","uniq","game","activeNodes","allNodes","activePlayers","players","filter","p","online","newNeighbors","id","set","console","log"],"mappings":"AAAAA,MAAM,CAACC,MAAP,CAAc;AAACC,EAAAA,YAAY,EAAC,MAAIA,YAAlB;AAA+BC,EAAAA,sBAAsB,EAAC,MAAIA;AAA1D,CAAd;;AAAO,SAASD,YAAT,CAAsBE,SAAtB,EAAiCC,MAAjC,EAAyC;AAC5C,QAAMC,SAAS,GAAG,EAAlB;AACA,MAAIC,OAAO,GAAGH,SAAS,CAACI,KAAV,CAAgB,GAAhB,CAAd;AAEAD,EAAAA,OAAO,CAACE,OAAR,CAAiBC,CAAD,IAAO;AACrB,UAAMC,UAAU,GAAGD,CAAC,CAACF,KAAF,CAAQ,GAAR,CAAnB;AACA,UAAMI,WAAW,GAAGP,MAAM,CAACQ,GAAP,CAAW,QAAX,CAApB;;AAEA,QAAID,WAAW,KAAKE,QAAQ,CAACH,UAAU,CAAC,CAAD,CAAX,CAA5B,EAA6C;AAC3CL,MAAAA,SAAS,CAACS,IAAV,CAAeJ,UAAU,CAAC,CAAD,CAAzB;AACD,KAFD,MAEO,IAAIC,WAAW,KAAKE,QAAQ,CAACH,UAAU,CAAC,CAAD,CAAX,CAA5B,EAA6C;AAClDL,MAAAA,SAAS,CAACS,IAAV,CAAeJ,UAAU,CAAC,CAAD,CAAzB;AACD;AACF,GATD;AAWA,SAAOK,CAAC,CAACC,IAAF,CAAOX,SAAP,EAAkB,IAAlB,CAAP;AACD;;AAEI,SAASH,sBAAT,CAAgCe,IAAhC,EAAsC;AACzC,QAAMC,WAAW,GAAG,EAApB;AACA,QAAMC,QAAQ,GAAG,EAAjB;AACA,QAAMC,aAAa,GAAGH,IAAI,CAACI,OAAL,CAAaC,MAAb,CAAoBC,CAAC,IAAIA,CAAC,CAACC,MAAF,KAAa,IAAb,IAAqB,CAACD,CAAC,CAACX,GAAF,CAAM,UAAN,CAA/C,CAAtB;AAEAQ,EAAAA,aAAa,CAACZ,OAAd,CAAuBe,CAAD,IAAO;AAC3BL,IAAAA,WAAW,CAACJ,IAAZ,WAAoBS,CAAC,CAACX,GAAF,CAAM,QAAN,CAApB;AACD,GAFD;AAIAK,EAAAA,IAAI,CAACI,OAAL,CAAab,OAAb,CAAsBe,CAAD,IAAO;AAC1B,QAAIA,CAAC,CAACC,MAAF,KAAa,IAAb,IAAqB,CAACD,CAAC